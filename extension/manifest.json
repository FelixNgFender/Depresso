{

	"manifest_version": 2,
	"name": "DeDepression",
	"description": "This extension is running the Python code.",
	"version": "1.0",

	"browser_action":
	{
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},

	"background": {
		"scripts": ["input.js"],
		"persistent": true
	},

	"permissions": [
		"activeTab",
		"tabs",
		"background",
		"https://ajax.googleapis.com/",
		"http://127.0.0.1"
	]
}